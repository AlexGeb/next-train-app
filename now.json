{
  "version": 2,
  "name": "next-train-monorepo",
  "builds": [
    { "src": "front/package.json", "use": "@now/static-build" },
    { "src": "back/*.py", "use": "@now/python" }
  ],
  "routes": [
    { "src": "/api", "dest": "/back/test.py" },
    { "src": "^/static/(.*)", "dest": "/front/static/$1" },
    { "src": ".*", "dest": "/front/index.html" }
  ],
  "alias": ["next-train"],
  "regions": ["bru1"]
}
